<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="IntelliWatch.UCTractBar"
	x:Name="UserControl"
	d:DesignWidth="1386" d:DesignHeight="80" MinWidth="500" MinHeight="80" >

	<Grid x:Name="LayoutRoot" VerticalAlignment="Bottom">
		<Slider Margin="0" Style="{DynamicResource SliderStyleRecord}"
                IsMoveToPointEnabled="True" MinHeight="80" />
	</Grid>
</UserControl>