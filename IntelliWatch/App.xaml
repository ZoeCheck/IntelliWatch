<Application x:Class="IntelliWatch.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="Window\MainWindow.xaml">
    <Application.Resources>
         
    	<ResourceDictionary>
    		<ResourceDictionary.MergedDictionaries>
    			<ResourceDictionary Source="Style/TabCntrolStyle.xaml"/>
    			<ResourceDictionary Source="Style/TreevViewStyle.xaml"/>
    			<ResourceDictionary Source="Style/TextBoxStyle.xaml"/>
                <ResourceDictionary Source="Style/ButtonStyle.xaml" />
                <ResourceDictionary Source="Style/RadioButtonStyle.xaml"/>
                <ResourceDictionary Source="Style/TrackBarStyle.xaml"/>
                <ResourceDictionary Source="Style/PathListBoxStyle.xaml"/>
                <ResourceDictionary Source="Style/CheckBoxStyle.xaml"/>
    		</ResourceDictionary.MergedDictionaries>
    		<SolidColorBrush x:Key="BrushMainWindow" Color="#FF036AC6"/>
    		<SolidColorBrush x:Key="BrushNull" Color="#CC000000"/>
    	</ResourceDictionary>
         
    </Application.Resources>
</Application>
